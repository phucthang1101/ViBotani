<!-- search product-->
<div style="position:fixed;bottom:2rem;left:0.5rem;z-index:100;width:25%;">
    <div id="search__header">
        <button class="btn btn-success" style="border-radius:50%;width:40px;height:40px;" id="search_open"><i class="fas fa-search"></i></button>
        <button class="btn d-none pb-0" style="color:#28a745" id="search_close"><i class="fas fa-times"></i></button>
    </div>
    <div class=" d-none" id="search_container">
        <input style="border:3px solid #28a745;border-radius:0;" type="text" class="mb-2 form-control" placeholder="Tìm kiếm sản phẩm" aria-label="Input group example" aria-describedby="btnGroupAddon">
        <div class="input-group mb-2">
            <select style="border:3px solid #28a745;border-radius:0;display:block;" type="text" class="form-control search__product">
                <option disabled selected>--Chọn dòng sản phẩm--</option>
                <option value="/san-pham-trang-da">Sản phẩm trắng da</option>
                <option value="#">Sản phẩm đẹp da</option>
                <option value="#">Mỹ phẩm</option>
                <option value="#">Sản phẩm đặc trị</option>
            </select>
        </div>
        <div class="input-group mb-2">
            <select style="border:3px solid #28a745;border-radius:0;display:block;" type="text" class="form-control search__product">
                <option disabled selected>--Chọn loại sản phẩm--</option>
                <option value="/san-pham-trang-da/trang-mat">Trắng mặt</option>
                <option value="/san-pham-trang-da/trang-cap-toc">Trắng cấp tốc</option>
                <option value="/san-pham-trang-da/trang-toan-than">Trắng toàn thân</option>
            </select>
        </div>
        <button class="btn btn-success" style="border-radius:0;"><i class="fas fa-search"></i></button>
    </div>
</div>
